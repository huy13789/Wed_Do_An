@model IEnumerable<Do_An_Wed.Models.DANHMUC>
@{
    ViewBag.Title = "Danhmuc";

}
<p class="lead" style="font-weight: 600; color: #212529"> DANH MỤC MỸ PHẨM</p>
<div class="list-group" style="font-size: 16px; font-weight: 600;">
    @foreach (var item in Model)
    {
        <a href="@Url.Action("SPTheodanhmuc", "Sanphams", new { id = item.MaDM })" class="list-group-item">
            @item.TenDM
        </a>

    }
</div>
    